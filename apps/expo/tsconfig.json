{
  "extends": [
    "@acme/tsconfig/base.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@env": [
        "./src/env.js"
      ]
    },
    "jsx": "react-native",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "types": [
      "nativewind/types"
    ],
    "esModuleInterop": true,
    "checkJs": false
  },
  "include": [
    "src",
    "*.ts",
    "**/*.tsx",
    "*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
